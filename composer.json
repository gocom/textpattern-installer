{
    "name": "textpattern/installer",
    "type": "composer-installer",
    "description": "Textpattern plugin and theme installer",
    "keywords": ["textpattern", "plugin", "theme", "txp", "installer"],
    "homepage": "https://github.com/gocom/textpattern-installer",
    "license": "GPL-2.0",
    "authors": [
    	{
    		"name": "Jukka Svahn",
    		"homepage": "http://rahforum.biz",
    		"role": "Developer"
    	}
    ],
    "support": {
    	"issues": "https://github.com/gocom/textpattern-installer/issues",
    	"source": "https://github.com/gocom/textpattern-installer"
    },
    "require": {
        "textpattern/lock": ">=4.4.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.5.*"
    },
    "autoload": {
        "psr-0": {
            "Textpattern\\Composer\\Installer\\": "src/"
        }
    },
    "extra": {
        "class": [
            "Textpattern\\Composer\\Installer\\Installer\\Manifest",
            "Textpattern\\Composer\\Installer\\Installer\\Package",
            "Textpattern\\Composer\\Installer\\Installer\\AdminTheme",
            "Textpattern\\Composer\\Installer\\Installer\\PublicTheme",
            "Textpattern\\Composer\\Installer\\Installer\\Textpack"
        ]
    }
}
